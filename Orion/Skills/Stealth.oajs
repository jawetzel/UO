function DoThing(){

		var playerStartingX = Player.X();
		var playerStartingY = Player.Y();
		var playerStartingZ = Player.Z();
		var moveToX = playerStartingX;
	while(true){
	
		if(Player.Y() === playerStartingY){
			moveToY = playerStartingY + 10
		}
		if(Player.Y() === playerStartingY + 10){
			moveToY = playerStartingY
		}
	
		Orion.WalkTo(playerStartingX, moveToY, playerStartingZ, 0, 0, 1, 0, 250)

	}

}